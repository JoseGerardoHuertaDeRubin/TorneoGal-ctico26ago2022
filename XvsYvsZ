function [fitresult, gof] = createFit(PosicionX, PosicionY, PosicionZ)
%CREATEFIT(POSICIONX,POSICIONY,POSICIONZ)
%  Create a fit.
%
%  Data for 'X vs Y vs Z' fit:
%      X Input: PosicionX
%      Y Input: PosicionY
%      Z Output: PosicionZ
%  Output:
%      fitresult : a fit object representing the fit.
%      gof : structure with goodness-of fit info.
%
%  See also FIT, CFIT, SFIT.

%  Auto-generated by MATLAB on 28-Aug-2022 01:00:09


%% Fit: 'X vs Y vs Z'.
[xData, yData, zData] = prepareSurfaceData( PosicionX, PosicionY, PosicionZ );

% Set up fittype and options.
ft = 'linearinterp';

% Fit model to data.
[fitresult, gof] = fit( [xData, yData], zData, ft, 'Normalize', 'on' );

% Plot fit with data.
figure( 'Name', 'X vs Y vs Z' );
h = plot( fitresult, [xData, yData], zData );
legend( h, 'X vs Y vs Z', 'PosicionZ vs. PosicionX, PosicionY', 'Location', 'NorthEast', 'Interpreter', 'none' );
% Label axes
xlabel( 'PosicionX', 'Interpreter', 'none' );
ylabel( 'PosicionY', 'Interpreter', 'none' );
zlabel( 'PosicionZ', 'Interpreter', 'none' );
grid on


